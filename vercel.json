{
  "version": 2,
  "builds": [
    { "src": "api/logements.js", "use": "@vercel/node" },
    { "src": "pages/**/*.html", "use": "@vercel/static" },
    { "src": "assets/**/*", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/logements.js" },
    { "src": "/(.*)", "dest": "/pages/$1" }
  ]
}
